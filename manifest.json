{
  "manifest_version": 3,
  "name": "SnapSSID",
  "version": "1.0",
  "description": "Grab dev session token for localhost",
  "permissions": ["cookies", "tabs", "activeTab", "<all_urls>"],
  "background": {
    "scripts": ["service-worker.js"]
  },
  "browser_action": {
    "default_title": "SnapSSID"
  },
  "icons": {
    "48": "icon.png"
  }
}
